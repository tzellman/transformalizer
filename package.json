{
  "name": "transformalizer",
  "version": "1.1.3",
  "description": "a bare bones node module for building JSON API v1.0 compliant payloads",
  "main": "./dist/index.js",
  "keywords": [
    "json-api",
    "json api",
    "json",
    "api",
    "serialize",
    "serializer",
    "transform",
    "transformalizer"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc-watch -p tsconfig.json",
    "clean": "rimraf dist coverage",
    "coverage": "nyc mocha -r ts-node/register test/tests/*.test.ts",
    "postversion": "npm run build",
    "prebuild": "npm run clean",
    "release": "npm run build && standard-version",
    "test": "mocha -r ts-node/register test/tests/**/*.test.ts",
    "lint": "eslint --max-warnings 0 ./lib"
  },
  "contributors": [
    {
      "name": "Chris Ludden",
      "url": "https://github.com/cludden"
    },
    {
      "name": "Jason Sites",
      "url": "https://github.com/jasonsites"
    },
    {
      "name": "Joshua Wynn",
      "url": "https://github.com/joshuarwynn"
    },
    {
      "name": "Dave Smith",
      "url": "https://github.com/davesmitty"
    },
    {
      "name": "Tom Zellman",
      "url": "https://github.com/tzellman"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GaiamTV/transformalizer"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/faker": "^4.1.12",
    "@types/lodash": "^4.14.154",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.9",
    "@types/qs": "^6.9.3",
    "@types/ramda": "^0.27.6",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "chai": "^3.5.0",
    "chance": "^1.1.6",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "faker": "^4.1.0",
    "json-pretty": "0.0.1",
    "lodash": "^4.17.15",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "qs": "^6.9.4",
    "ramda": "^0.27.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "standard-version": "^8.0.0",
    "ts-node": "^8.10.2",
    "tsc-watch": "^4.2.8",
    "typescript": "^3.9.3"
  },
  "dependencies": {},
  "nyc": {
    "reporter": ["text", "lcov"],
    "tempDir": "coverage"
  }
}
